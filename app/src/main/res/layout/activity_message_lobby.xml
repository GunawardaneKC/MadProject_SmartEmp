<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MessageLobby"
    android:background="@drawable/backgroundimageworld">


    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="60dp"
        android:layout_height="49dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="664dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/baseline_arrow_back_ios_new_24"
        android:contentDescription="BACK" />

    <Button
        android:id="@+id/ClientProfileBtn"
        android:layout_width="251dp"
        android:layout_height="50dp"
        android:layout_marginTop="288dp"
        android:backgroundTint="@color/DBlue"
        android:onClick="AddVacancys"
        android:text="Add vacancy"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/OrderRedirect"
        android:layout_width="251dp"
        android:layout_height="50dp"
        android:backgroundTint="@color/DBlue"
        android:onClick="sentMessages"
        android:text="Added vacancies"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ClientProfileBtn"
        app:layout_constraintVertical_bias="0.099" />

</androidx.constraintlayout.widget.ConstraintLayout>